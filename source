local function WTDE()
	while true do end
end
local function openTab(tab:ScrollingFrame)
	for i,tob in MainFrame:GetChildren() do
		if tob:IsA("ScrollingFrame") and tob ~= tab then
			tob.Visible = false
		end
	end
	if tab.Visible == false then
		scroll.Visible = false
		tab.Visible = true	
	else
		scroll.Visible = true
		tab.Visible = false			
	end
end
DM.MouseButton1Click:Connect(function()
	openTab(DebugSroll)
end)
CS.MouseButton1Click:Connect(function()
	openTab(CustomSkinList)
end)
