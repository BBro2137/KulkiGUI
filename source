	SavedCommunicator.Event:Connect(function()
		FULLSTOP()
	end)	
end

workspace:FindFirstChild("Map"):FindFirstChild("Ingame").ChildAdded:Connect(function(coolkid)
	if coolkid:IsA("Model") and coolkid.Name:lower() == "shockwave" and canRunUpdateLoop == true and options.MovePredictor == true then
		--local cl = coolkid:Clone()
		--print(cl.Root.Size)
		--cl.Root.Anchored = true
		--cl.Root.CFrame = Client.Character.PrimaryPart.CFrame
		--cl.Parent = workspace
		local cl = coolkid:Clone()
		local hitbox = cl.Root
		local ogPos = hitbox.Position
		hitbox.Transparency = 0
		hitbox.Material = Enum.Material.Neon
		hitbox.Color = Color3.fromRGB(255,0,0)
		hitbox.CanCollide = false
		hitbox.CanTouch = false
		hitbox.CanQuery = false
		hitbox.Size = Vector3.new(hitbox.Size.X,hitbox.Size.Y,hitbox.Size.Z+1000)
		hitbox.CFrame = hitbox.CFrame + hitbox.CFrame.LookVector*500
		hitbox.Parent = workspace
		task.wait(4.5)
		hitbox:Destroy()
	elseif coolkid:IsA("Model") and coolkid.Name:lower() == "swords" and canRunUpdateLoop == true and options.MovePredictor == true then
		local cl = coolkid:Clone()
		local hitbox = cl:FindFirstChild("Core")
		local ogPos = hitbox.Position
		hitbox.Transparency = 0
		hitbox.Material = Enum.Material.Neon
		hitbox.Color = Color3.fromRGB(255,0,0)
		hitbox.CanCollide = false
		hitbox.CanTouch = false
		hitbox.CanQuery = false
		hitbox.Size = Vector3.new(hitbox.Size.X,hitbox.Size.Y,hitbox.Size.Z+1000)
		hitbox.CFrame = hitbox.CFrame + hitbox.CFrame.LookVector*-500
		hitbox.Parent = workspace
		task.wait(4.5)
		hitbox:Destroy()
	elseif coolkid:IsA("Model") and coolkid.Name:lower() == "spike" and coolkid:FindFirstChild("Spike") and options.AssetLoadReplacement == true and canRunUpdateLoop == true then
		local mesh = coolkid:FindFirstChild("Spike")
		local cloneHitbox = Instance.new("Part")
		cloneHitbox.Color = mesh.Color
		cloneHitbox.CanCollide = false
		cloneHitbox.Anchored = true
		cloneHitbox.CanTouch = false
		cloneHitbox.Material = mesh.Material
		cloneHitbox.Size = Vector3.new(mesh.Size.X,15,mesh.Size.Z)
		cloneHitbox.CFrame = mesh.CFrame
		cloneHitbox.Name = "SpikeAssetReplacement"
		cloneHitbox.Parent = workspace
		task.wait(14.1)
		cloneHitbox:Destroy()
	elseif options.AssetLoadReplacement == true and coolkid.Name == "1x1x1x1Zombie" then
		task.wait(0.5)
		coolkid:FindFirstChild("Torso").Transparency = 0
		coolkid:FindFirstChild("Head").Transparency = 0
		coolkid:FindFirstChild("Left Arm").Transparency = 0
		coolkid:FindFirstChild("Left Leg").Transparency = 0
		coolkid:FindFirstChild("Right Arm").Transparency = 0
		coolkid:FindFirstChild("Right Leg").Transparency = 0
	end
	if coolkid.Name == "Map" and coolkid:IsA("Model") and options.AssetLoadReplacement == true then
		print("searching")
		task.wait(5)
		local x = 0
		for i,bar in coolkid:GetDescendants() do
			x+=1
			if x >= 500 then
				task.wait()
			end
			if bar.Name == "barrier" and bar.Transparency > 0.9 then
				bar.Transparency = 0.7
				bar.Color = Color3.fromRGB(255,0,0)
			end
		end
	end
	if coolkid:FindFirstChildWhichIsA("Humanoid") and coolkid:FindFirstChild("Left Arm") and coolkid:FindFirstChild("Left Leg") and options.showMinions == true and coolkid.Name ~= "007n7" then
		local high = Instance.new("Highlight")
		if coolkid.Name == "1x1x1x1Zombie" then
			high.FillTransparency = 0.5
			high.FillColor = Color3.fromRGB(128, 191, 128)
			high.OutlineColor = Color3.fromRGB(128, 191, 128)	
		else
			high.FillColor = Color3.fromRGB(255,255,255)
			high.OutlineColor = Color3.fromRGB(255,255,255)		
		end
		high.Parent = coolkid
		high.Name = "FilthyESP77"
	end
	if DEBUGoptions.IngameCHILDADDED == true and canRunUpdateLoop == true then
		print(coolkid.Name .. "   " .. coolkid:GetFullName())
		if #coolkid:GetDescendants()>0 then
			for i,d in coolkid:GetDescendants() do
				print(d.Name)
				print(d.ClassName)
			end			
		end
	end
end)
workspace:FindFirstChild("Map"):FindFirstChild("Ingame").DescendantAdded:Connect(function(coolkid)
