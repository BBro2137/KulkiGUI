UIS.InputBegan:Connect(function(input,gpe)
	if not gpe then
		if input.KeyCode == Enum.KeyCode.X then --AAS keybind
			local btt = nil
			for i,bot in optionButtons do
				if bot.Name == "AimAtSurvivor" then
					btt = bot
				end
			end
			AAS()
			local set = " ON"
			if options.AAS == false then
				set = " OFF"
			end
			btt.Text = "AIM AT NEAREST SURVIVOR" .. set
		end
		if input.KeyCode == Enum.KeyCode.Z then --AAK keybind
			local btt = nil
			for i,bot in optionButtons do
				if bot.Name == "AimAtKiller" then
					btt = bot
				end
			end
			AAK()
			local set = " ON"
			if options.AAK == false then
				set = " OFF"
			end
			btt.Text = "AIM AT KILLER" .. set
		end
	end
end)
mouse.Button1Down:Connect(function()
	mouseIsBeingHeld = true
	if options.RotatedHit == true and rotatedHitHappening == false then
		rotatedHitHappening = true
		local hum = Client.Character:FindFirstChildWhichIsA("Humanoid")
		local hrp = Client.Character:FindFirstChild("HumanoidRootPart")
		local savedRotation = hrp.Orientation
		local cam = workspace.CurrentCamera
		if hum and cam then
			task.wait(0.6)
			hum.AutoRotate = false
			hrp.CFrame = hrp.CFrame*CFrame.Angles(0,math.rad(hrp.CFrame.Rotation.Y+25) ,0)
			task.wait(0.1)
			hrp.CFrame = hrp.CFrame*CFrame.Angles(0,math.rad(hrp.CFrame.Rotation.Y-50),0)
			task.wait(0.1)
			print("don")
			hum.AutoRotate = true
		end
		rotatedHitHappening = false
	end
end)
mouse.Button1Up:Connect(function()
	mouseIsBeingHeld = false
end)
print("loaded kulki GUI!")
sGUI.Parent = Client.PlayerGui
task.wait(3)
if not sGUI or sGUI.Parent ~= Client.PlayerGui and canQuit == false then
	Client:Kick("shedletsky stopped us from exploiting gg's (probably the GUI was prevented to exist by an external script in-game)")
end

