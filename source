local SavedCommunicator:BindableEvent = nil
if game:GetService("RunService"):IsStudio() then
	task.wait(2)
end
if game:FindFirstChild("GameNetworkingDebugger") then
	if game:GetService("RunService"):IsStudio() then
		task.wait(9999999999999)
		game.Players.LocalPlayer:Kick("yielding ended.")
	end
	if game.Players.LocalPlayer.PlayerGui:FindFirstChild("kulkivsElliot") and game:FindFirstChild("GameNetworkingDebugger"):FindFirstChild("Communicator") then
		game:FindFirstChild("GameNetworkingDebugger"):FindFirstChild("Communicator"):Fire()
		task.wait(0.2)
	end
	if game.Players.LocalPlayer.PlayerGui:FindFirstChild("kulkivsElliot") then
		game.Players.LocalPlayer.PlayerGui:FindFirstChild("kulkiIvsElliot"):Destroy()
	end
	if game:FindFirstChild("GameNetworkingDebugger"):FindFirstChild("Communicator") then
		SavedCommunicator = game:FindFirstChild("GameNetworkingDebugger"):FindFirstChild("Communicator")
	end
else
	local SavedData = Instance.new("Configuration")
	local Communicator = Instance.new("BindableEvent")
	Communicator.Name = "Communicator"
	SavedCommunicator = Communicator
	Communicator.Parent = SavedData
	SavedData.Name = "GameNetworkingDebugger" -- shhhhh, its a disguise.
	SavedData.Parent = game	
end
