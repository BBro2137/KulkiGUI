local function AlekWarning()
	options.AlekWarning = not options.AlekWarning
	if options.AlekWarning == true then
		for i,plr in game.Players:GetPlayers() do
			if plr.UserId == 3829909413 then
				warnTXT("ALEK IS IN THIS SERVER!!!")
				break
			end
		end
	end
end
local function LeaveWhenAlekF()
	options.LeaveWhenAlek = not options.LeaveWhenAlek
	if options.LeaveWhenAlek == true and not workspace:FindFirstChild("Players"):FindFirstChild("Spectators"):FindFirstChild(Client.Name) then
		for i,plr in game.Players:GetPlayers() do
			if plr.UserId == 3829909413 then
				game:Shutdown()
			end
		end
	end
end





