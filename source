		if DEBUGoptions.IngameDESCENDANTADDED == true then
			print(coolkid.Name .. "   " .. coolkid:GetFullName())
		end	
	end
end)
workspace:FindFirstChild("Players"):FindFirstChild("Killers").ChildAdded:Connect(function(killer)
	if killer.Name == "JohnDoe" and skinStatus.BigDjohnDoe == true and canRunUpdateLoop == true then
		killer:WaitForChild("HumanoidRootPart").Anchored = true
		task.wait(0.5)
		local D25 = Instance.new("Model")
		D25.Name = "D" .. tostring(math.random(-20,-5)) .. "ez"
		local connect = Instance.new("Weld")
		local L = Instance.new("Part")
		local B1 = Instance.new("Part")
		local CBL = Instance.new("Weld")
		B1.CanCollide = false
		B1.CanTouch = false
		B1.CanQuery = false
		L.CanCollide = false
		L.CanTouch = false
		L.CanQuery = false
		L.Massless = true
		B1.Massless = true
		L.Material = Enum.Material.SmoothPlastic
		B1.Material = Enum.Material.SmoothPlastic
		CBL.C0 = CFrame.new(Vector3.new(0,1,250)) 
		CBL.Part1 = B1
		CBL.Part0 = L
		CBL.C1 = CFrame.new(Vector3.new(2,1,0))
		L.Color = Color3.fromRGB(255, 255, 0)
		B1.Color = Color3.fromRGB(255, 255, 0)
		local CBL2 = CBL:Clone()
		B1.Shape = Enum.PartType.Ball
		B1.Size = Vector3.new(5,5,5)
		local B2 = B1:Clone()
		CBL2.Part1 = B2
		CBL2.C1 = CFrame.new(Vector3.new(-2,1,0))
		L.Size = Vector3.new(2,2,500)

		connect.C0 = CFrame.new(Vector3.new(0,1,250))
		connect.Part0 = L
		connect.Part1 = killer:WaitForChild("Torso")
		connect.Parent = killer

		CBL.Parent = D25
		L.Parent = D25
		CBL2.Parent = D25
		B1.Parent = D25	
		B2.Parent = D25

		D25.Parent = workspace
		task.wait(1)
		killer:WaitForChild("HumanoidRootPart").Anchored = false
	elseif killer.Name == "c00lkidd" and skinStatus.GyatCoolkid == true and canRunUpdateLoop == true then
		killer:WaitForChild("HumanoidRootPart").Anchored = true
		task.wait(0.5)
		local D25 = Instance.new("Model")
		D25.Name = "D" .. tostring(math.random(-20,-5)) .. "ez"
		local connect = Instance.new("Weld")
		local L = Instance.new("Part")
		local B1 = Instance.new("Part")
		local CBL = Instance.new("Weld")
		B1.CanCollide = false
		B1.CanTouch = false
		B1.CanQuery = false
		L.CanCollide = false
		L.CanTouch = false
		L.CanQuery = false
		L.Massless = true
		B1.Massless = true
		L.Material = Enum.Material.SmoothPlastic
		L.Transparency = 1
		B1.Material = Enum.Material.SmoothPlastic
		CBL.C0 = CFrame.new(Vector3.new(0,1,10)) 
		CBL.Part1 = B1
		CBL.Part0 = L
		CBL.C1 = CFrame.new(Vector3.new(0.5,1,-1))
		local CBL2 = CBL:Clone()
		B1.Shape = Enum.PartType.Ball
		B1.Size = Vector3.new(3,3,3)
		B1.Color = Color3.fromRGB(255,0,0)
		local B2 = B1:Clone()
		CBL2.Part1 = B2
		CBL2.C1 = CFrame.new(Vector3.new(-0.5,1,-1))
		L.Size = Vector3.new(2,2,20)
		
		connect.C0 = CFrame.new(Vector3.new(0,1,10))
		connect.Part0 = L
		connect.Part1 = killer:WaitForChild("Torso")
		connect.Parent = killer
	
		CBL.Parent = D25
		L.Parent = D25
		CBL2.Parent = D25
		B1.Parent = D25	
		B2.Parent = D25
		
		D25.Parent = workspace
		task.wait(1)
		killer:WaitForChild("HumanoidRootPart").Anchored = false
	end
end)
local warnFrame = Instance.new("Frame")
warnFrame.Parent = sGUI
warnFrame.Size = UDim2.new(0.377, 0,0.365, 0)
warnFrame.Position = UDim2.new(0.311, 0,0.635, 0) 
warnFrame.BackgroundTransparency = 0.5
warnFrame.BackgroundColor3 = Color3.fromRGB(0,0,0)
warnFrame.BorderSizePixel = 0
warnFrame.Visible = false
local warnTxt = Instance.new("TextLabel")
warnTxt.Parent = warnFrame
warnTxt.Size = UDim2.new(1,0,1,0)
warnTxt.Position = UDim2.new(0,0,0,0)
warnTxt.TextColor3 = Color3.fromRGB(255,0,0)
warnTxt.TextScaled = true
warnTxt.TextWrapped = true

local function warnTXT(text:string)
	if warnFrame.Visible == true then
		task.wait(4)
	end
	warnFrame.Visible = true
	warnTxt.Text = text
	task.wait(4)
	warnFrame.Visible = false
end
local function oo7n7predict(survivor)
	if survivor.Name == "007n7" and options.OO7n7tpPrediction == true and canRunUpdateLoop == true then
		survivor.ChildAdded:Connect(function(multi)
			if multi.Name == "c00lgui" and canRunUpdateLoop == true and options.OO7n7tpPrediction == true then
				local Cover = Instance.new("Part")
				Cover.Name = "cover"
				Cover.Size = Vector3.new(20,20,20)
				local stop = false
				local function updatePos()
					if not Cover.Parent or Cover.Parent ~= workspace then
						Cover.Parent = workspace
					end
					if survivor and survivor:FindFirstChild("HumanoidRootPart") then
						local furthestSpawn = {
							["spawn"] = nil,
							["dist"] = 0
						}
						for i,spawnP:SpawnLocation in workspace:FindFirstChild("Map"):FindFirstChild("Ingame"):FindFirstChild("Map"):FindFirstChild("SpawnPoints"):FindFirstChild("Survivors"):GetChildren() do
							local mag = (spawnP.Position-survivor.HumanoidRootPart.Position).Magnitude
							if mag > furthestSpawn.dist then
								furthestSpawn.spawn = spawnP
								furthestSpawn.dist = mag
							end
						end
						Cover.Position = furthestSpawn.spawn.Position
					else
						Cover:Destroy()
						stop = true
					end
				end
				Cover.CanCollide = false
				Cover.CanQuery = false
				Cover.CastShadow = false
				Cover.Transparency = 0.7
				local highLight = Instance.new("Highlight")
				highLight.Parent = Cover
				highLight.FillColor = Color3.fromRGB(255,255,255)
				highLight.OutlineColor = Color3.fromRGB(0,0,255)
				highLight.Enabled = true
				local bil = Instance.new("BillboardGui")
				local txt = Instance.new("TextLabel")
				bil.Size = UDim2.new(0, 150,0, 50)
				bil.Name = "FilthyDisplay77"
				bil.AlwaysOnTop = true
				txt.Parent = bil
				txt.BackgroundTransparency = 1
				txt.TextColor3 = Color3.fromRGB(255, 162, 0)
				txt.TextScaled = true
				txt.FontFace.Bold = true
				txt.Size = UDim2.new(1,0,1,0)
				txt.Text = game.Players:GetPlayerFromCharacter(survivor).DisplayName .. " teleports here, in 6s."
				bil.Parent = Cover
				Cover.Anchored = true
				
				local x = 5
				local z = 25
				task.spawn(function()
					repeat
						if stop == true then
							break
						end
						task.wait(0.1)
						updatePos()
					until x <= 0 
				end)
				repeat
					if stop == true then
						break
					end
					task.wait(1)
					if survivor and txt and game.Players:GetPlayerFromCharacter(survivor) then
						txt.Text = game.Players:GetPlayerFromCharacter(survivor).DisplayName .. " teleports here, in " .. tostring(x) .. "s"
					else
						break
					end
					x-=1
				until x<=0
				if survivor and txt and game.Players:GetPlayerFromCharacter(survivor) then
					txt.Text = game.Players:GetPlayerFromCharacter(survivor).DisplayName .. " teleports here!"
				end
				if stop == false then
					task.wait(2)
				end
				if Cover then
					Cover:Destroy()
				end
			end
		end)
	end
end
workspace:FindFirstChild("Players"):FindFirstChild("Survivors").ChildAdded:Connect(function(survivor)
	if survivor.Name:lower() == "chance" and skinStatus.NaziChance == true and canRunUpdateLoop == true then
		survivor:WaitForChild("HumanoidRootPart").Anchored = true
		task.wait(0.5)
		local D25 = Instance.new("Model")
		D25.Name = "D" .. tostring(math.random(-20,-5)) .. "ez"
		local connect = Instance.new("Weld")
		local Mid = Instance.new("Part")
		Mid.CanCollide =false
		Mid.CanTouch = false
		Mid.CanQuery = false
		Mid.Size = Vector3.new(2,10,2)
		local SideMid = Mid:Clone()
		local short1 = Instance.new("Part")
		short1.CanCollide =  false
		short1.CanTouch = false
		short1.CanQuery = false
		short1.Size = Vector3.new(2,5,2)
		local short2 = short1:Clone()
		local short3 = short1:Clone()
		local short4 = short1:Clone()
		
		local midSideW = Instance.new("Weld")
		local MidShortDown = Instance.new("Weld")
		local MidShortLeft = Instance.new("Weld")
		local MidShortRight = Instance.new("Weld")
		local MidShortUp = Instance.new("Weld")
		
		connect.Parent = D25
		Mid.Parent = D25
		short1.Parent = D25
		short2.Parent = D25
		short3.Parent = D25
		short4.Parent = D25
		SideMid.Parent = D25
		midSideW.Parent = D25
		MidShortRight.Parent = D25
		MidShortUp.Parent = D25
		MidShortLeft.Parent = D25
		MidShortDown.Parent = D25
		
		midSideW.C1 = CFrame.Angles(math.rad(0),math.rad(0),math.rad(90))
		midSideW.Part0 = Mid
		midSideW.Part1 = SideMid
		connect.Part0 = Mid
		connect.Part1 = survivor:WaitForChild("HumanoidRootPart")
		connect.C0 = CFrame.new(0,-1,-2)
		
		MidShortDown.Part0 = Mid
		MidShortDown.Part1 = short3
		MidShortDown.C1 = CFrame.new(-4,-2.55,0) * CFrame.Angles(math.rad(0),math.rad(0),math.rad(90))
		
		MidShortLeft.Part0 = Mid
		MidShortLeft.Part1 = short2
		MidShortLeft.C1 = CFrame.new(4,2.55,0)
		
		MidShortRight.Part0 = Mid
		MidShortRight.Part1 = short4
		MidShortRight.C1 = CFrame.new(-4,-2.55,0) 
		
		MidShortUp.Part0 = Mid
		MidShortUp.Part1 = short2
		MidShortUp.C1 = CFrame.new(4,2.6,0) * CFrame.Angles(math.rad(0),math.rad(0),math.rad(90))
		
		D25.Parent = workspace
		task.wait(1)
		survivor:WaitForChild("HumanoidRootPart").Anchored = false
	end
	oo7n7predict(survivor)
end)
