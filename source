local GENDISPLAYS = {}

local optionButtons = {}

local function createOption(name,text,connectTo,realName,risky,appearIn)
	local btt = Instance.new("TextButton")
	btt.Name =  name
	btt.Text = text
	btt.TextScaled = true
	btt.Size = UDim2.new(1, 0,0.0085, 0)
	btt.BackgroundColor3 = Color3.fromRGB(115, 115, 115)
	if risky == true then
		btt.BackgroundColor3 = Color3.fromRGB(150,0,0)
		btt.LayoutOrder = 999
	end
	btt.MouseButton1Click:Connect(function()
		connectTo()
		local set = " ON"
		if options[realName] == false then
			set = " OFF"
		end
		btt.Text = text .. set
	end)
	if not appearIn or appearIn:lower() == 'main' or appearIn:lower() == "scroll" then
		btt.Parent = scroll
	elseif appearIn and appearIn:lower()=="warningconfig" then
		btt.Parent = WarningScroll
	elseif appearIn and appearIn:lower()=="generatorconfig" then
		btt.Parent = GeneratorScroll
	elseif appearIn and appearIn:lower()=="otheroptions" then
		btt.Parent = OtherOptionsScroll
	end
	
	table.insert(optionButtons,btt)
end
local function createDebugOption(name,text,connectTo,realName)
	local btt = Instance.new("TextButton")
	btt.Name =  name
	btt.Text = text
	btt.TextScaled = true
	btt.Size = UDim2.new(1, 0,0.0085, 0)
	btt.BackgroundColor3 = Color3.fromRGB(115, 115, 115)
	btt.MouseButton1Click:Connect(function()
		connectTo()
		local set = " ON"
		if DEBUGoptions[realName] == false then
			set = " OFF"
		end
		btt.Text = text .. set
	end)
	btt.Parent = DebugSroll
	table.insert(optionButtons,btt)
end
local function createCustomSkin(name,text,realName,connectTo)
	local btt = Instance.new("TextButton")
	btt.Name =  "OPT" .. name
	btt.Text = text .. " OFF"
	btt.TextScaled = true
	btt.Size = UDim2.new(1, 0,0.0085, 0)
	btt.BackgroundColor3 = Color3.fromRGB(115, 115, 115)
	btt.MouseButton1Click:Connect(function()
		skinStatus[realName] = not skinStatus[realName]
		local set = " ON"
		if skinStatus[realName] == false then
			set = " OFF"
		end
		btt.Text = text .. set
		if connectTo ~= nil then
			connectTo()
		end
	end)
	btt.Parent = CustomSkinList
	table.insert(optionButtons,btt)
end

createCustomSkin("BDJD","BIG D JOHN DOE","BigDjohnDoe")
