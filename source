	if plr.UserId == 3829909413 and options.AlekWarning == true then
		warnTXT("ALEK JOINED THIS SERVER BEWAREEEEE!!!!!!")
	end
	if options.LeaveWhenAlek == true and not workspace:FindFirstChild("Players"):FindFirstChild("Spectators"):FindFirstChild(Client.Name) then
		if plr.UserId == 3829909413 then
			game:Shutdown()
		end
	end
	plr.Chatted:Connect(function(msg,chatter)
		sl(msg,plr)
	end)
end)
